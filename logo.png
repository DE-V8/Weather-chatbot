<?xml version="1.0" encoding="UTF-8"?>
<svg width="100" height="100" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
  <!-- Circular gradient background -->
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4158D0;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#C850C0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFCC70;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Main circle -->
  <circle cx="50" cy="50" r="45" fill="white" opacity="0.9" />
  
  <!-- Weather elements -->
  <g transform="translate(50, 50)">
    <!-- Sun -->
    <circle cx="0" cy="0" r="15" fill="#FFC107" />
    <circle cx="0" cy="0" r="22" fill="none" stroke="#FFC107" stroke-width="2" stroke-dasharray="2 4" />
    
    <!-- Cloud -->
    <path d="M-5,12 a10,10 0 1,0 25,0 a8,8 0 1,0 -25,0" fill="#3498db" opacity="0.9" />
    
    <!-- AI element - circuit-like pattern -->
    <path d="M-22,-22 L-18,-18 M-15,-25 L-12,-20 M-25,-15 L-20,-12" stroke="#4158D0" stroke-width="1.5" />
    <path d="M22,-22 L18,-18 M15,-25 L12,-20 M25,-15 L20,-12" stroke="#C850C0" stroke-width="1.5" />
    <circle cx="-20" cy="-20" r="2" fill="#4158D0" />
    <circle cx="20" cy="-20" r="2" fill="#C850C0" />
  </g>
  
  <!-- Neural Network Lines -->
  <path d="M30,70 Q50,85 70,70" stroke="url(#grad)" stroke-width="1.5" fill="none" />
  <path d="M25,65 Q50,90 75,65" stroke="url(#grad)" stroke-width="1.5" fill="none" />
  
  <!-- Pulse effect -->
  <circle cx="50" cy="50" r="48" fill="none" stroke="url(#grad)" stroke-width="2">
    <animate attributeName="r" from="48" to="52" dur="3s" repeatCount="indefinite" />
    <animate attributeName="opacity" from="0.8" to="0" dur="3s" repeatCount="indefinite" />
  </circle>
</svg> 